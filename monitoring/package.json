{
  "name": "tts-performance-monitoring",
  "version": "1.0.0",
  "description": "Performance monitoring and load testing suite for Ticket Tracking System",
  "scripts": {
    "load-test": "artillery run load-tests/artillery-config.yml",
    "load-test:smoke": "artillery run load-tests/smoke-test.yml",
    "load-test:stress": "artillery run load-tests/stress-test.yml",
    "metrics:collect": "node metrics/collector.js",
    "metrics:analyze": "node metrics/analyzer.js",
    "report:generate": "node metrics/report-generator.js",
    "monitor:docker": "./docker/monitor.sh",
    "db:analyze": "node database/analyze.js"
  },
  "dependencies": {
    "artillery": "^2.0.0",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "pg": "^8.11.0",
    "sqlite3": "^5.1.6",
    "chart.js": "^4.4.0",
    "express-prometheus-middleware": "^1.2.0",
    "prom-client": "^15.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
